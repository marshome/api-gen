                    ]
                },
                {
                    "name": "riceIndices",
                    "desc": "The encoded local, lexicographically-sorted list indices, using a\nGolomb-Rice encoding. Used for sending compressed removal indices. The\nremoval indices (uint32) are sorted in ascending order, then delta encoded\nand stored as encoded_data.",
                    "type": "ref",
                    "refType": "RiceDeltaEncoding"
                }
