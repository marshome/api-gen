                        },
                        {
                            "name": "UNWANTED_SOFTWARE",
                            "desc": "Unwanted software threat type."
                        },
                        {
                            "name": "POTENTIALLY_HARMFUL_APPLICATION",
                            "desc": "Potentially harmful application threat type."
                        }
                    ]
                },
                {
                    "name": "removals",
                    "desc": "A set of entries to remove from a local threat type's list. In practice,\nthis field is empty or contains exactly one ThreatEntrySet.",
